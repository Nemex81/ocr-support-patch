### ============================================
### TYPES - COUNTY VIEW
### ============================================

types CountyViewTypes
{
	### WIDGET BUILDING ITEM ###
	type widget_building_item = widget {
		datacontext = "[GUIBuildingItem.GetBuilding]"
		size = { 100 100 }
		allow_outside = yes

		button_standard = {
			size = { 100 100 }
			onclick = "[GUIBuildingItem.OnClick]"
			down = "[GUIBuildingItem.IsHighlighted]"
			enabled = "[GUIBuildingItem.IsBuildingButtonEnabled]"
			tooltip = "[GUIBuildingItem.GetTooltip]"

			using = tooltip_ne

			vbox = {
				margin = { 3 3 }
				spacing = 3

				### BUILDING ICON ###
				icon_building = {
					name = "building_icon"
					size = { 90 70 }
					texture = "[GUIBuildingItem.GetCurrentOrConstrucingBuilding.GetTypeIcon]"

					icon = {
						name = "has_variants"
						visible = "[Building.HasVariants]"
						parentanchor = top|left
						size = { 30 30 }
						position = { -5 -5 }
						texture = "gfx/interface/icons/symbols/icon_variants.dds"
					}

					blockoverride "button_frames" {
						gfxtype = togglepushbuttongfx
						effectname = "NoHighlight"
						upframe = 1
						uphoverframe = 2
						uppressedframe = 3
						downframe = 2
						disableframe = 4
					}
				}

				### BUILDING NAME & LEVEL ###
				hbox = {
					margin_top = -5
					spacing = 3

					text_single = {
						text = "[GUIBuildingItem.GetCurrentOrConstrucingBuilding.GetTypeName]"
						default_format = "#high"
						max_width = 70
						fontsize_min = 10
					}

					text_single = {
						visible = "[GUIBuildingItem.HasLevel]"
						text = "[GUIBuildingItem.GetCurrentOrConstrucingBuilding.GetLevel]"
						default_format = "#high;bold"
						min_width = 20
						align = center
						fontsize = 16

						background = {
							using = Background_Area_Dark
							margin = { 3 0 }
						}
					}
				}

				### WARNINGS ###
				hbox = {
					spacing = 3

					warning_icon = {
						name = "wrong_type"
						datacontext = "[HoldingView.GetHolder]"
						visible = "[And( Not( Holding.IsEmpty ), Not( Holding.IsValidForLesseeOrHolder ) )]"
						size = { 25 25 }
						tooltip = "HOLDING_VIEW_WRONG_HOLDING_TYPE"
					}

					warning_icon = {
						name = "reduced_by_fixable_situation"
						visible = "[And( ObjectsEqual( GUIBuildingItem.Self, Holding.Self ), GUIBuildingItem.IsBuildingDisabled )]"
						size = { 25 25 }
					}
				}

				expand = {}
			}

			### CONSTRUCTION PROGRESS ###
			widget = {
				visible = "[GUIBuildingItem.IsConstructing]"
				size = { 100 8 }
				parentanchor = bottom|hcenter

				progressbar_standard = {
					name = "building_upgrade_progressbar"
					parentanchor = center
					size = { 106 10 }
					value = "[GUIBuildingItem.GetConstructingProgress]"
					alwaystransparent = yes
				}
			}
		}
	}

	### BUILDING IN VASSAL WARNING ###
	type building_in_vassal_warning_hbox = hbox {
		datacontext = "[HoldingView.GetCountyTitle]"
		datacontext = "[Title.GetHolder]"
		visible = "[And( Not( Character.IsLocalPlayer ), Title.HasFaction )]"
		layoutpolicy_horizontal = expanding
		margin = { 0 10 }

		background = {
			using = Background_Area
			margin = { 10 0 }

			modify_texture = {
				using = Status_Bad
				texture = "gfx/interface/component_masks/mask_rough_edges.dds"
				spriteType = Corneredtiled
				spriteborder = { 10 10 }
				blend_mode = alphamultiply
			}
		}

		icon = {
			size = { 40 40 }
			texture = "gfx/interface/icons/symbols/icon_warning.dds"
		}

		text_multi = {
			text = "BUILDING_IN_VASSAL_WARNING"
			autoresize = yes
			max_width = 600
			margin = { 10 5 }
		}
	}

	### ICON BUILDING TAB ###
	type icon_building_tab = icon {
		texture = "[Building.GetType.GetTabIcon]"

		blockoverride "button_frames" {
			frame = "[BoolTo1And2(Building.IsConstructing)]"
		}

		state = {
			name = _mouse_enter
			on_start = "[PdxGuiWidget.FindChild('building_progressbar').TriggerAnimation('mouse_enter')]"
		}

		widget = {
			name = "building_progressbar"
			parentanchor = bottom|hcenter
			size = { 60 8 }
			position = { 0 5 }

			progressbar_standard = {
				name = "building_upgrade_progressbar"
				visible = "[Building.IsConstructing]"
				size = { 60 8 }
				value = "[Building.GetConstructionProgress]"

				state = {
					name = mouse_enter
					alpha = 1
					duration = 0.2
				}

				state = {
					name = _mouse_leave
					alpha = 0
					duration = 0.3
				}
			}
		}
	}

	### ICON BUILDING ###
	type icon_building = icon {
		texture = "[Building.GetTypeIcon]"
		size = { 60 60 }
		alwaystransparent = yes

		framesize = { 120 120 }

		blockoverride "button_frames" {
			frame = "[BoolTo1And2(Building.IsConstructing)]"
		}
	}

	### FERTILITY BAR ###
	type fertility_bar = widget {
		size = { 150 60 }

		vbox = {
			margin = { 20 5 }
			margin_bottom = 10
			spacing = 3

			hbox = {
				margin = { 5 0 }
				spacing = 5

				icon = {
					name = "grey_fertility_icon"
					size = { 30 30 }
					texture = "gfx/interface/icons/icon_fertility.dds"
					framesize = { 40 40 }
					frame = 4
					tooltip = "[Title.GetAverageFertilityDesc]"

					block "grey_fertility_icon_visibility" {
						visible = "[Title.IsAverageFertilityBetween( '(CFixedPoint)0.0', '(CFixedPoint)1.1' )]"
					}

					block "bar_tooltip" {}
				}

				icon = {
					name = "green_fertility_icon"
					size = { 30 30 }
					texture = "gfx/interface/icons/icon_fertility.dds"
					framesize = { 40 40 }
					frame = 3
					tooltip = "[Title.GetAverageFertilityDesc]"

					block "green_fertility_icon_visibility" {
						visible = no
					}

					block "bar_tooltip" {}
				}

				icon = {
					name = "yellow_fertility_icon"
					size = { 30 30 }
					texture = "gfx/interface/icons/icon_fertility.dds"
					framesize = { 40 40 }
					frame = 2
					tooltip = "[Title.GetAverageFertilityDesc]"

					block "yellow_fertility_icon_visibility" {
						visible = no
					}

					block "bar_tooltip" {}
				}

				icon = {
					name = "red_fertility_icon"
					size = { 30 30 }
					texture = "gfx/interface/icons/icon_fertility.dds"
					framesize = { 40 40 }
					frame = 1
					tooltip = "[Title.GetAverageFertilityDesc]"

					block "red_fertility_icon_visiblity" {
						visible = no
					}

					block "bar_tooltip" {}
				}

				button_round = {
					datacontext = "[GetPlayer]"
					visible = "[And( CountyFertilityBar.ShowIncreaseFertilityButton, Character.HasPerk( 'erudition_lifestyle_3_perk' ) )]"
					size = { 30 30 }

					enabled = "[CountyFertilityBar.CanIncreaseFertility]"
					onclick = "[CountyFertilityBar.IncreaseFertility]"
					tooltip = "[CountyFertilityBar.GetIncreaseFertilityTooltip]"

					block "button_tooltip" {}

					button_icon = {
						texture = "gfx/interface/icons/flat_icons/plus.dds"
						size = { 20 20 }
						parentanchor = center
						alwaystransparent = yes
					}
				}
			}

			progressbar_standard = {
				size = { 120 14 }
				value = "[CountyFertilityBar.GetCurrentFertilityProgress]"
			}
		}
	}

	### COUNTY FERTILITY TOOLTIP ###
	type county_fertility_tooltip_container = container {
		tooltipwidget = {
			using = DefaultTooltipBackground

			vbox = {
				set_parent_size_to_minimum = yes
				margin = { 15 10 }
				spacing = 3

				text_label_center = {
					text = "HOLDING_VIEW_COUNTY_FERTILITY_TOOLTIP_HEADER"
					default_format = "#T"
					max_width = 350
				}

				text_label_center = {
					layoutpolicy_horizontal = expanding
					text = "HOLDING_VIEW_COUNTY_FERTILITY_TOOLTIP_CURRENT"
					max_width = 350
					margin_bottom = 5
				}

				divider_light = {
					layoutpolicy_horizontal = expanding
				}

				vbox_value_breakdown_tooltip_content = {
					datacontext = "[CountyDataBreakdowns.AccessFertility( County.Self )]"
					layoutpolicy_horizontal = expanding
				}

				expand = {}
			}
		}
	}

	### COUNTY CONTROL TOOLTIP ###
	type county_control_tooltip_container = container {
		tooltipwidget = {
			widget_value_breakdown_tooltip = {
				datacontext = "[CountyDataBreakdowns.AccessControl( County.Self )]"

				blockoverride "values_explanation" {
					text_single = {
						layoutpolicy_horizontal = expanding
						text = "COUNTY_CONTROL_EXPLANATION"
						max_width = 350
					}
				}
			}
		}
	}

	### COUNTY DEVELOPMENT TOOLTIP ###
	type county_development_tooltip_container = container {
		tooltipwidget = {
			widget_value_breakdown_tooltip = {
				datacontext = "[CountyDataBreakdowns.AccessDevelopment( County.Self )]"

				blockoverride "values_explanation" {
					text_single = {
						layoutpolicy_horizontal = expanding
						text = "COUNTY_DEVELOPMENT_EXPLANATION"
						max_width = 350
					}
				}
			}
		}
	}

	### HOLDING TOOLTIP CLICK ###
	type holding_tooltip_click = container {
		using = GeneralTooltipSetup

		container = {
			tooltipwidget = {
				using = DefaultTooltipBackground

				flowcontainer = {
					direction = vertical
					margin = { 20 10 }
					margin_bottom = 15

					text_single = {
						text = "[Holding.GetProvince.GetTitle.GetNameNoTooltip]"
						default_format = "#T"
						max_width = 400
						margin_bottom = 10
					}

					text_multi = {
						text = "[Holding.GetTypeBreakdown]"
						autoresize = yes
						max_width = 400
					}
				}
			}
		}
	}
}


