								layoutpolicy_horizontal = expanding
								datacontext = [County.GetCountyOccupant]
								visible = [County.IsFullyOccupied]
								blockoverride "text" {
									raw_text = "County occupied by [Character.GetPrimaryTitle.GetNameNoTierNoTooltip]."
